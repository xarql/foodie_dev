<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.mapper.ItemsSpecMapper">
    <select id="selectItemsSpec" resultType="com.imooc.pojo.ItemsSpecDO">
        select *
        from items_spec
        where item_id=#{itemId}
    </select>
    <select id="selctIttemsSpecById" resultType="com.imooc.pojo.ItemsSpecDO">
        select *
        from items_spec
        where id=#{id}
    </select>
</mapper>